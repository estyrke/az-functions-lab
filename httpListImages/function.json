{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get"],
      "route": "/images"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "imageDocuments",
      "databaseName": "imageApp",
      "collectionName": "image",
      "connectionStringSetting": "COSMOS_CONNECTION_STRING"
    }
  ],
  "scriptFile": "../dist/httpListImages/index.js"
}
